
<div id="languages-list">
  <% @languages.names.each_with_index do |name, index| %>
    <div class="filename"
         id="language_<%= index %>"
         data-language="<%= name %>"
         data-index="<%= index %>"
         data-test-index="<%= @languages.tests_indexes[index][0] %>">
      <%= name %>
    </div>
  <% end %>
</div>